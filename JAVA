import java.io.IOException;
import java.util.Scanner;

public class Main {
 
    public static void main(String[] args) throws IOException {
        
        Scanner read = new Scanner(System.in);
        int N = read.nextInt();
        for (int i = 0; i < N - 1; i++) {
            System.out.println("Ho ");
        }
        System.out.println("Ho!");
    }
 
}
